{
  "program_name": "Alternating Power and Hypertrophy Split",
  "weekly_structure": {
    "description": "3 training days per week with alternating leg focus",
    "order_cycle": [
      "Legs_1",
      "Upper_Body_1",
      "Upper_Body_2",
      "Legs_2",
      "Upper_Body_1",
      "Upper_Body_2"
    ],
    "notes": "Continue cycling in this order. Each new week picks up where the previous one ended."
  },
  "workouts": {
    "Legs_1": {
      "focus": "Explosive / Power / Athletic",
      "exercises": [
        {
          "name": "Box Jumps",
          "sets": 5,
          "reps": 3
        },
        {
          "name": "Speed Back Squat",
          "sets": 6,
          "reps": 3,
          "intensity": "60% 1RM"
        },
        {
          "name": "Romanian Deadlift (Explosive Concentric)",
          "sets": 4,
          "reps": 5,
          "tempo": "Fast up / 3s eccentric"
        },
        {
          "name": "Walking Lunges (Explosive Step)",
          "sets": 3,
          "reps": "6 each leg"
        },
        {
          "name": "Broad Jumps",
          "sets": 4,
          "reps": 3
        },
        {
          "name": "Standing Calf Jumps",
          "sets": 4,
          "reps": 10
        },
        {
          "name": "Hanging Leg Raises",
          "sets": 3,
          "reps": "10-12"
        }
      ]
    },
    "Legs_2": {
      "focus": "Hypertrophy / Strength",
      "exercises": [
        {
          "name": "Back Squat",
          "sets": 5,
          "reps": 5
        },
        {
          "name": "Romanian Deadlift",
          "sets": 4,
          "reps": "6-8"
        },
        {
          "name": "Leg Press",
          "sets": 3,
          "reps": "10-12"
        },
        {
          "name": "Bulgarian Split Squat",
          "sets": 3,
          "reps": "8 each leg"
        },
        {
          "name": "Seated or Lying Leg Curl",
          "sets": 3,
          "reps": "10-12"
        },
        {
          "name": "Standing Calf Raise",
          "sets": 4,
          "reps": "12-15",
          "tempo": "2s pause at top"
        },
        {
          "name": "Wall Sit",
          "sets": 2,
          "duration": "45-60s"
        }
      ]
    },
    "Upper_Body_1": {
      "focus": "Strength / Compound Bias",
      "exercises": [
        {
          "name": "Barbell Bench Press",
          "sets": 5,
          "reps": 5
        },
        {
          "name": "Weighted Pull-Ups",
          "sets": 4,
          "reps": "6-8"
        },
        {
          "name": "Standing Overhead Press",
          "sets": 4,
          "reps": 6
        },
        {
          "name": "Chest-Supported Row",
          "sets": 4,
          "reps": 8
        },
        {
          "name": "Incline Dumbbell Press",
          "sets": 3,
          "reps": "8-10"
        },
        {
          "name": "Face Pulls",
          "sets": 3,
          "reps": "12-15"
        },
        {
          "name": "Cable Triceps Pushdown",
          "sets": "2-3",
          "reps": "10-12"
        },
        {
          "name": "EZ-Bar or Dumbbell Curls",
          "sets": "2-3",
          "reps": "10-12"
        }
      ]
    },
    "Upper_Body_2": {
      "focus": "Hypertrophy / Volume / Shoulder and Arms",
      "exercises": [
        {
          "name": "Incline Dumbbell Bench Press",
          "sets": 4,
          "reps": "8-12"
        },
        {
          "name": "Single-Arm Lat Pulldown or One-Arm Cable Row",
          "sets": 4,
          "reps": "10-12 each side"
        },
        {
          "name": "Seated Dumbbell Shoulder Press",
          "sets": 3,
          "reps": "10-12"
        },
        {
          "name": "Lateral Raises",
          "sets": 4,
          "reps": "12-15"
        },
        {
          "name": "Rear Delt Flyes",
          "sets": 3,
          "reps": "12-15"
        },
        {
          "name": "EZ-Bar or Dumbbell Curls",
          "sets": 3,
          "reps": "10-12"
        },
        {
          "name": "Overhead Cable Triceps Extension",
          "sets": 3,
          "reps": "10-12"
        },
        {
          "name": "Hammer Curls",
          "sets": 2,
          "reps": "12-15"
        },
        {
          "name": "Rope Triceps Pushdown",
          "sets": 2,
          "reps": "12-15"
        }
      ]
    }
  }
}
